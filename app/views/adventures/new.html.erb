<h1>Create your new adventure</h1>

 <%= form_for @adventure do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :recommendation %>
    <%= f.text_area :recommendation %>

    <%= f.label :is_wishlist? %>
    <%= f.check_box :is_wishlist? %>

    <%= f.submit%>
<% end %>