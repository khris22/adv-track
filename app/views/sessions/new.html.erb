<h1>Log In</h1>
<div class="container">
<%= flash[:error] %>

<%= form_for @user, url: login_path do |f| %>

    <p><%= f.label :username %>
    <%= f.text_field :username, required: true %></p>

    <p><%= f.label :password %>
    <%= f.password_field :password, required: true %></p>

    <p><%= f.submit 'Log In'%></p>
<% end %>
</div>