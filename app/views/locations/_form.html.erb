<%= form_for @location do |f| %>

    <%= render partial: 'layouts/error_messages', locals: {object: @location} %>

    <%= f.label :city %>
    <%= f.text_field :city %>

    <%= f.label :state %>
    <%= f.text_field :state %>

    <%= f.label :description%>
    <%= f.text_area :description %>

    <%= f.submit%>
<% end %>